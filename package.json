{
  "name": "Splash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pod:install": "pod install --project-directory=ios",
    "test": "jest --watch --coverage=false",
    "test-coverage": "jest --watch --coverage=true",
    "test:android": "jest --watch --coverage=false -c ./jest/android.config.js --root-dir=.",
    "test:ios": "jest --watch --coverage=false -c ./jest/ios.config.js --root-dir=.",
    "test:ci": "jest --debug --verbose --ci",
    "test:cs": "jest -u --coverage=true",
    "typecheck": "tsc -p tsconfig.spec.json",
    "typecheck:no-tests": "tsc -p tsconfig.json",
    "typecheck:watch": "tsc -p tsconfig.spec.json --watch",
    "updateSnapshots": "jest -u --coverage=false",
    "debugger": "open \"rndebugger://set-debugger-loc?host=localhost\"",
    "prettier:check": "prettier --check \"**/*.{js,ts,jsx,tsx,css,md}\" --config .prettierrc",
    "prettier:write": "prettier --write \"**/*.{js,ts,jsx,tsx,css,md}\" --config .prettierrc"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-navigation/bottom-tabs": "5.11.1",
    "@react-navigation/devtools": "5.1.17",
    "@react-navigation/native": "5.8.9",
    "@react-navigation/stack": "5.12.6",
    "apollo-boost": "0.4.9",
    "graphql": "15.4.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-gesture-handler": "1.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.3",
    "react-native-localization": "2.1.6",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.15.0",
    "react-native-svg": "12.1.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "styled-components": "5.2.1",
    "typesafe-redux-helpers": "0.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/runtime": "7.12.5",
    "@jtomaszewski/babel-plugin-styled-components": "1.10.8-beta.0",
    "@testing-library/react-hooks": "3.4.2",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.56",
    "@types/react-native": "0.63.35",
    "@types/react-redux": "7.1.11",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.13.0",
    "eslint-config-react-native": "4.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native": "3.10.0",
    "husky": "^4.3.7",
    "isomorphic-fetch": "3.0.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-enzyme": "7.1.2",
    "jest-expo": "39.0.0",
    "jest-fetch-mock": "3.0.3",
    "jest-react-hooks-shallow": "1.4.1",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "react-native-flipper": "0.64.0",
    "react-native-svg-transformer": "0.14.3",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npm run lint && npm run typecheck && npm run prettier:check"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
