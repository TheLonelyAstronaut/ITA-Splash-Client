{
  "name": "Splash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pod:install": "pod install --project-directory=ios",
    "test": "jest --coverage=false",
    "test:watch": "jest --watch --coverage=false",
    "test-coverage": "jest --watch --coverage=true",
    "test:android": "jest --watch --coverage=false -c ./jest/android.config.js --root-dir=.",
    "test:ios": "jest --watch --coverage=false -c ./jest/ios.config.js --root-dir=.",
    "test:ci": "jest --debug --verbose --ci",
    "test:cs": "jest -u --coverage=true",
    "typecheck": "tsc -p tsconfig.spec.json",
    "typecheck:no-tests": "tsc -p tsconfig.json",
    "typecheck:watch": "tsc -p tsconfig.spec.json --watch",
    "updateSnapshots": "jest -u --coverage=false",
    "debugger": "open \"rndebugger://set-debugger-loc?host=localhost\"",
    "prettier:check": "prettier --check \"**/*.{js,ts,jsx,tsx,css,md}\" --config .prettierrc",
    "prettier:write": "prettier --write \"**/*.{js,ts,jsx,tsx,css,md}\" --config .prettierrc"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.2",
    "@gorhom/animated-tabbar": "^2.1.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/styled-components": "^5.1.7",
    "@types/styled-components-react-native": "^5.1.0",
    "i18n-js": "^3.8.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-flash-message": "^0.1.22",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-localize": "^2.0.1",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-svg": "^12.1.0",
    "react-native-track-player": "^2.0.0-rc13",
    "react-native-vector-icons": "^7.1.0",
    "react-navigation": "^4.4.3",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "typesafe-redux-helpers": "^0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-redux": "7.1.11",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.13.0",
    "eslint-config-react-native": "4.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native": "3.10.0",
    "husky": "^4.3.7",
    "jest": "^25.1.0",
    "lint-staged": "10.5.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "2.1.2",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npm run lint && npm run typecheck && npm run prettier:check"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
